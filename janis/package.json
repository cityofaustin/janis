{
  "name": "react-static-example-basic",
  "private": true,
  "scripts": {
    "serve": "serve dist -p 3000",
    "start": "NODE_PATH=./src CMS_API='https://joplin.herokuapp.com/api/graphql' CMS_MEDIA='https://joplin-austin-gov.s3.amazonaws.com/media' npm-run-all -p watch-css start-js",
    "build": "NODE_PATH=./src CMS_API='https://joplin.herokuapp.com/api/graphql' CMS_MEDIA='https://joplin-austin-gov.s3.amazonaws.com/media' npm-run-all build-css build-js && cp -r dist/* _dist/",
    "start-js": "react-static start",
    "build-js": "react-static build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "axios": "^0.18.0",
    "babel-plugin-react-intl-auto": "^1.7.0",
    "body-scroll-lock": "^2.6.1",
    "browser-locale": "^1.0.3",
    "classnames": "^2.2.6",
    "downshift": "^3.2.10",
    "flexboxgrid-sass": "^8.0.5",
    "graphql-request": "^1.8.2",
    "html-react-parser": "^0.7.0",
    "intl": "^1.2.5",
    "js-cookie": "^2.2.0",
    "moment-timezone": "^0.5.25",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.2",
    "react-accessible-accordion": "^3.0.0",
    "react-dom": "^16.8.2",
    "react-ga": "^2.5.7",
    "react-intl": "^2.8.0",
    "react-load-script": "^0.0.6",
    "react-static": "^7.0.0",
    "react-static-plugin-reach-router": "^7.0.0",
    "react-static-plugin-sitemap": "^7.0.0",
    "react-static-plugin-source-filesystem": "^7.0.0",
    "uswds": "1"
  },
  "devDependencies": {
    "babel-eslint": "9.x",
    "eslint": "5.x",
    "eslint-config-react-app": "^3.0.8",
    "eslint-config-react-tools": "1.x.x",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "serve": "10.1.2"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
