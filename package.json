{
  "name": "janis",
  "version": "1.0.1",
  "description": "Public facing frontend public side of austin.gov using React components.",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "browser-locale": "^1.0.3",
    "flexboxgrid-sass": "https://github.com/hugeinc/flexboxgrid-sass.git",
    "graphql-request": "^1.4.1",
    "intl": "^1.2.5",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.0",
    "query-string": "^5.0.1",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-ga": "^2.4.1",
    "react-hot-loader": "^3.1.3",
    "react-intl": "^2.4.0",
    "react-load-script": "^0.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-static": "5.8.5",
    "react-universal-component": "^2.8.1",
    "storybook": "^1.0.0",
    "ua-parser-js": "^0.7.17",
    "uswds": "^1.4.5"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^3.3.15",
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addon-viewport": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.15",
    "babel-core": "^6.26.0",
    "babel-plugin-react-intl": "^2.4.0",
    "eslint-config-react-tools": "1.x.x",
    "prettier": "1.12.1",
    "serve": "^6.1.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js && cp -r dist/* _dist/",
    "build-langs": "babel intl.buildlangs.js | node",
    "start-js": "react-static start",
    "build-js": "react-static build",
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "storybook": "start-storybook -p 80",
    "build-storybook": "build-storybook"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
